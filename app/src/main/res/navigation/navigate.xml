<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:id="@+id/navigate"
    app:startDestination="@id/homeFragment">

    <fragment
        android:id="@+id/homeFragment"
        android:name="com.example.lab4.createFragment.HomeFragment"
        android:label="HomeFragment" >
        <action
            android:id="@+id/action_homeFragment_to_addSheduleFragment"
            app:destination="@id/addSheduleFragment" />
        <action
            android:id="@+id/action_homeFragment_to_editSheduleFragment"
            app:destination="@id/editSheduleFragment" />
    </fragment>
    <fragment
        android:id="@+id/addSheduleFragment"
        android:name="com.example.lab4.createFragment.AddSheduleFragment"
        android:label="AddSheduleFragment" >
        <action
            android:id="@+id/action_addSheFragment_to_homeFragment"
            app:destination="@id/homeFragment" />
    </fragment>
    <fragment
        android:id="@+id/editSheduleFragment"
        android:name="com.example.lab4.createFragment.EditSheduleFragment"
        android:label="EditSheduleFragment" >
        <action
            android:id="@+id/action_editSheduleFragment_to_homeFragment"
            app:destination="@id/homeFragment" />

        <argument
            android:name="shedule"
            app:argType="model.Shedule"
            app:nullable="true"/>

    </fragment>

</navigation>